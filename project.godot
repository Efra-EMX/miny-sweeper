config_version=5

[application]

config/name="Miny Sweeper"
config/version="1.0.0"
config/tags=PackedStringArray("2d")
run/main_scene="uid://bg248w7ij17mf"
config/use_custom_user_dir=true
config/project_settings_override="user://settings.cfg"
config/features=PackedStringArray("4.4", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/image="res://assets/ui/logos/EMX Logo.png"
config/icon="res://icon.png"

[aseprite]

import/import_plugin/default_automatic_importer="SpriteFrames"

[autoload]

AudioManager="*res://autoloads/AudioManager.tscn"
PopUpManager="*res://autoloads/PopUpManager.gd"
SceneManager="*res://autoloads/SceneManager.tscn"
Settings="*res://autoloads/Settings.gd"
DebugController="*res://scripts/debug/DebugController.gd"
FocusManager="*res://autoloads/FocusManager.gd"
Global="*res://autoloads/Global.gd"
CallableBuffer="*res://autoloads/CallableBuffer.gd"
TurnManager="*res://autoloads/TurnManager/TurnManager.tscn"
Navigation="*res://autoloads/Navigation.gd"

[display]

window/size/viewport_width=320
window/size/viewport_height=180
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor]

naming/default_signal_callback_name="on_{signal_name}"
naming/default_signal_callback_to_self_name="on_{signal_name}"
version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg", "res://addons/Todo_Manager/plugin.cfg", "res://addons/gdfxr/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://assets/": "blue",
"res://autoloads/": "red",
"res://builds/": "teal",
"res://scenes/": "orange",
"res://scripts/": "purple"
}

[global_group]

pages="A group of Page nodes"
player=""
enemy=""
entity=""
character=""

[gui]

theme/custom="res://assets/ui/themes/Axial/Axial-theme.tres"
theme/custom_font="res://assets/ui/themes/Axial/AXIAL.ttf"

